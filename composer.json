{
  "name": "bruno-barros/w.eloquent-bus",
  "description": "w.eloquent Command Bus Pattern.",
  "keywords": [
    "Laravel",
    "framework",
    "WordPress",
    "Command Bus"
  ],
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Bruno Barros",
      "email": "brunodanca@gmail.com"
    }
  ],
  "type": "project",
  "autoload": {
    "psr-4": {
      "Weloquent\\Bus\\": "src/Bus"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Weloquent\\Bus\\Tests\\": "src/tests"
    }
  },
  "require": {
    "php": ">=5.4",
    "bruno-barros/w.eloquent-framework": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.2.*",
    "mockery/mockery": "dev-master"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "extra": {
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
